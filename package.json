    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^17.0.0",
    "husky": "^9.1.7",
    "jsdom": "^27.4.0",
    "prettier": "^3.7.4",
    "sass": "^1.97.2",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.52.0",
    "vite": "^7.2.4",
    "vitest": "^4.0.17"
